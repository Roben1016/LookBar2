<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.roshine.lookbar.book" >

    <application
        android:name="debug.BookApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/book_app_name"
        android:supportsRtl="true"
        android:theme="@style/common_AppTheme">
        <activity android:name="debug.ModuleBookActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activity.BookDetailActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".ui.activity.BookTagActivity"
            android:screenOrientation="portrait"/>
    </application>
</manifest>
